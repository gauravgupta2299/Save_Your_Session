{ 
  "manifest_version": 2,

  "name": "Save your Sessions Plugin",
  "description": "This extension will save your current session",
  "version": "1.0",
  "background":{
  	"scripts":["background.js"]},
  "browser_action": {
   "default_icon": "S1-icon.png",
   "default_title": "Save This Session"
  },
  "permissions": [
   "background",
   "tabs",
   "bookmarks"
  // "http://*/*",
  // "https://*/*"
   ]
}
